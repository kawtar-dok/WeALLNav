@model IEnumerable<WeALLNav.Models.Societe>
@{
    ViewBag.Title = "Societes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="hero-wrap hero-wrap-2 js-fullheight" style="background-image: url(../../images/bgb2.jpg);" data-stellar-background-ratio="0.5">
    <div class="overlay"></div>
    <div class="container">
        <div class="row no-gutters slider-text js-fullheight align-items-end justify-content-start">
            <div class="col-md-9    pb-5">
                <p class="breadcrumbs"><span class="mr-2"><a href="index.html">Home ></a></span> <span>Our Companie ></span></p>
                <h1 class="mb-3 bread"> Our Companies</h1>
            </div>
        </div>
    </div>
</section>
<section>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name_STE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telephone_Ste)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.E_mail_Ste)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name_STE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Telephone_Ste)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.E_mail_Ste)
                </td>
                <td>

                    @Html.ActionLink("Details", "Details", new { id = item.No_Ste }) 

                </td>
            </tr>
        }

    </table>
</section>
